<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Apolo</title>

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&amp;subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">

    @Styles.Render("~/Assets/plugins/bootstrap/css/bootstrap.css")
    @Styles.Render("~/Assets/css/real-materialize.css")
    @RenderSection("Styles", false)
    <style>
        .body-patient {
            background: url("data:image/svg+xml;base64,");
        }
        .patient-board {
            position: absolute;
            left: 5%;
            top: 6%;
            width: 90%;
            height: 88%;
            background-color: white;
        }
        .welcome {
            margin: 30px 30px 30px 30px;
            font-size: 40px;
            display: block;
            color: #616161;
        }
        .welcome small {
            font-size: 30px;
            color: #616161;
        }
    </style>
</head>
<body class="body-patient">


    <section class="content">
        <div class="container-fluid">
            @RenderBody()
        </div>
    </section>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Assets/js/real-materialize.js")
    @Scripts.Render("~/Assets/plugins/jquery-countdown/js/jquery.countdown.js")
    @RenderSection("scripts", required: false)
</body>
</html>
